			category="it.xsemantics.dsl.wizards.category/it.xsemantics.project.Examples"
			icon="icons/genproject.gif" 
			project="true">
			<description>Featherweight Java (with cached type system) implemented with Xsemantics. You must import the Xsemantics FJ Example first.</description>
		</wizard>
	</extension>

   <extension
         point="org.eclipse.emf.common.ui.examples">
      <example
            pageImage="icons/genproject.gif"
            wizardID="it.xsemantics.examples.NewXsemanticsFjExampleWizard">
         <projectDescriptor
               contentURI="contents/it.xsemantics.example.fj.zip"
               description="Parser, Serializer, Linker, etc."
               name="it.xsemantics.example.fj">
         </projectDescriptor>
         <projectDescriptor
               contentURI="contents/it.xsemantics.example.fj.ui.zip"
               description="A smart editor for the language."
               name="it.xsemantics.example.fj.ui">
         </projectDescriptor>
         <projectDescriptor
               contentURI="contents/it.xsemantics.example.fj.tests.zip"
               description="A test suite for the language."
               name="it.xsemantics.example.fj.tests">
         </projectDescriptor>
      </example>
   </extension>

