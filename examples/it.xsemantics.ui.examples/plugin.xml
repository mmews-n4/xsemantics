<?eclipse version="3.4"?>
<plugin>
	<extension point="org.eclipse.ui.newWizards">
		<category id="it.xsemantics.ui.Examples" 
				  name="Xsemantics Examples" 
				  parentCategory="org.eclipse.ui.Examples" />
	</extension>
	<extension point="org.eclipse.ui.newWizards">
		<category id="it.xsemantics.project.Examples" 
				  name="Examples" 
				  parentCategory="it.xsemantics.dsl.wizards.category" />
	</extension>

	<extension point="org.eclipse.ui.newWizards">
		<wizard	id="it.xsemantics.examples.NewXsemanticsFjExampleWizard" 
			name="Xsemantics FJ Example" 
			class="org.eclipse.emf.common.ui.wizard.ExampleInstallerWizard" 
			category="org.eclipse.ui.Examples/it.xsemantics.ui.Examples"
			icon="icons/genproject.gif" 
			project="true">
			<description>Featherweight Java implemented with Xsemantics</description>
		</wizard>
	</extension>

	<extension point="org.eclipse.ui.newWizards">
		<wizard	id="it.xsemantics.examples.NewXsemanticsFjExampleWizard" 
			name="Xsemantics FJ Example" 
			class="org.eclipse.emf.common.ui.wizard.ExampleInstallerWizard" 
			category="it.xsemantics.dsl.wizards.category/it.xsemantics.project.Examples"
			icon="icons/genproject.gif" 
			project="true">
			<description>Featherweight Java implemented with Xsemantics</description>
		</wizard>
	</extension>

